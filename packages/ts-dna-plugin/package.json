{
  "name": "@chialab/ts-dna-plugin",
  "version": "3.0.0",
  "description": "A typescript transformer plugin for ttsc.",
  "keywords": [
    "typescript",
    "dna",
    "jsx"
  ],
  "homepage": "https://github.com/chialab/dna-tools/",
  "bugs": {
    "url": "https://github.com/chialab/dna-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chialab/dna-tools.git",
    "directory": "packages/ts-dna-plugin/"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "devDependencies": {
    "@chialab/manifest-analyzer-dna-plugin": "^3.0.0",
    "@custom-elements-manifest/analyzer": "^0.6.0",
    "custom-elements-manifest": "^2.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.0.0"
  }
}
