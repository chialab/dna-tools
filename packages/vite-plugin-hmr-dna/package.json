  "description": "Hot module replacement for DNA components and Vite.",
  "keywords": [
    "vite",
    "hmr",
    "dna"
  ],
  "homepage": "https://github.com/chialab/dna-tools/",
  "bugs": {
    "url": "https://github.com/chialab/dna-tools/issues"
  },
  "repository": {
    "type": "git",
