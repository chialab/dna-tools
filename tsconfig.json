{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "allowJs": true,
        "checkJs": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "composite": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "isolatedModules": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "strictBindCallApply": true,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [],
    "references": [
      {
        "path": "./packages/dna-storybook-renderer"
      },
      {
        "path": "./packages/dna-storybook-vite"
      }
    ]
}
